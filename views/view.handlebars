<div id="shopCar-view-container" class="container">
  <div class="row text-center">
    <a class="back-link main-color" href="/">Voltar</a>
    <div id="img-container" class="col-md-12">
      <img src="/img/car.svg" alt="Car">
    </div>
    <div class="col-md-12">
      <h1 class="main-color">{{ShopCar.dataValues.titulo}}</h1>
      <p class="bold">Aprecie com moderação</p>
      <p class="shopCar-desc">{{ShopCar.dataValues.marca}}</p>
      <p class="shopCar-desc">{{ShopCar.dataValues.modelo}}</p>
      <p class="shopCar-desc">{{ShopCar.dataValues.ano}}</p>
      <p class="shopCar-desc">{{ShopCar.dataValues.cor}}</p>
      <p class="shopCar-desc">{{ShopCar.dataValues.caracteristica}}</p>
      <p><span class="bold">O valor é só:</span> R${{ShopCar.dataValues.valor}}</p>
    </div>
  </div>
</div>